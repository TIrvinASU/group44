   <children>
      <Text layoutX="44.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please Enter ASURITE ID for Payment:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Text>
      <TextField layoutX="76.0" layoutY="55.0" onKeyTyped="#setAsuIdText" promptText="ASURITE ID" />
